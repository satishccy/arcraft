{
  "name": "arcraft",
  "version": "0.0.27",
  "description": "JavaScript SDK for ARC's utils in Algorand for both Node.js and browser environments",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": {
    ".": "dist/browser/arcraft.min.js",
    "crypto": false
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/satishccy/arcraft.git"
  },
  "dependencies": {
    "@filebase/client": "^0.0.5",
    "algosdk": "^3.3.1",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "form-data": "^4.0.0",
    "multiformats": "^9.9.0"
  },
  "optionalDependencies": {
    "mime-types": "^3.0.1"
  },
  "devDependencies": {
    "@types/json-bigint": "^1.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "assert": "^2.0.0",
    "concurrently": "^6.2.0",
    "es-abstract": "^1.18.3",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-tsdoc": "^0.2.11",
    "express": "^4.19.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^9.0.0",
    "mock-http-server": "^1.4.3",
    "prettier": "^3.2.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.3.1",
    "tsx": "^4.7.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.3.3",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "concurrently \"webpack --config webpack.config.js\" \"tsc -p tsconfig-esm.json\" \"tsc -p tsconfig-cjs.json\"",
    "docs": "typedoc --options typedoc.config.json",
    "docs:dev": "typedoc --options typedoc.config.json --watch --preserveWatchOutput",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write ."
  },
  "author": "satishccy",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
